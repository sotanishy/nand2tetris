/**
 * 8-way And: 
 * out = (in[0] and in[1] and ... and in[7])
 */

CHIP And8Way {
    IN in[8];
    OUT out;

    PARTS:
    And(a=in[0], b=in[1], out=c1);
    And(a=in[2], b=in[3], out=c2);
    And(a=in[4], b=in[5], out=c3);
    And(a=in[6], b=in[7], out=c4);
    And(a=c1, b=c2, out=d1);
    And(a=c3, b=c4, out=d2);
    And(a=d1, b=d2, out=out);
}
